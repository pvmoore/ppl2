
import core.intrinsics

public ////////////////////////////////////////

testCalls = {
    a = {
        return 0
    }
    c = { int a, int b ->
        return 5
    }

    //a()   // ambiguous
    //b(3)  // ambiguous
    assert 2 == b(3 as int) // b(int) exact match
    assert 3 == b(3.1)      // b(float)
    assert 3 == b(p1=3.1)
    assert 4 == b(1,2)
    assert 4 == b(p1=1, p2=3.1)
    assert 4 == b(p2=3.1, p1=1)
    assert 5 == c(7,8)
    assert 5 == c(a=7, b=8)
    assert 5 == c(b=7, a=9)
}

private ///////////////////////////////////////

a = {
    return 1
}
b = { int p1 ->
    return 2
}
b = { float p1 ->
    return 3
}
b = { int p1, float p2 ->
    return 4
}
