         imp.A a
         imp.A::B ab
         imp.A::B::C abc
         imp.A::B::C::D abcd
         
         imp.TA<int> ta
         imp.TA<float>::TB<int> tab
         imp.TA<double>::TB<long>::TC<int> tabc
         imp.TA<int>::TB<int>::TC<float>::TD<int> tabcd
    }
    testAccess()
    testImportAlias()
}
